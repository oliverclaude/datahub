namespace com.linkedin.ssis

import com.linkedin.common.Urn

/**
 * Key for a SSIS Data Flow
 */
@Aspect = {
  "name": "ssisDataFlowKey"
}
record SsisDataFlowKey {
  /**
  * Standardized ssis package urn representing the package for the flow
  */
  @Searchable = {
    "fieldName": "ssisPackage",
    "fieldType": "URN_PARTIAL",
    "queryByDefault": false
  }
  @Relationship = {
    "name": "DFIsPartOf",
    "entityTypes": [ "ssisPackage" ]
  }
  ssisPackage: Urn

  /**
  * Unique Identifier of the SSIS Data Flow
  */
  @Searchable = {
    "fieldType": "WORD_GRAM",
    "enableAutocomplete": true
  }
  dataFlowId: string
}
